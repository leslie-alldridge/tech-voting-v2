{
  "name": "boilerplate-react-webpack",
  "version": "0.1.1",
  "description": "Tech voting",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "webpack",
    "dev": "nodemon --watch server server/index & webpack --watch",
    "start": "node server/index",
    "test": "ava  -v  --serial 'test/**/*.test.js'",
    "watch": "nodemon -e js,jsx -x npm test",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "enzyme": "^2.8.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.15.2",
    "bulma-extensions": "^4.0.0",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "jsdom": "^11.0.0",
    "jsonwebtoken": "^7.4.3",
    "jsx-loader": "^0.13.2",
    "jwt-decode": "^2.2.0",
    "knex": "^0.20.1",
    "lodash": "^4.17.11",
    "nodemailer": "^5.0.0",
    "passport": "^0.4.0",
    "pg": "^6.2.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^4.0.4",
    "superagent": "^3.7.0",
    "webpack": "^2.3.3"
  }
}
